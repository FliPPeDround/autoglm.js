# AutoGLM.js Configuration
# Copy this file to .env and fill in your values

# Required: API Key from https://open.bigmodel.cn/
AUTOGLM_API_KEY=your_api_key_here

# Required: Model API Base URL
AUTOGLM_BASE_URL=https://open.bigmodel.cn/api/paas/v4/

# Required: Model name
AUTOGLM_MODEL=autoglm-phone

# Optional: Maximum reasoning steps (default: 100)
# AUTOGLM_MAX_STEPS=100

# Optional: Language preference (cn/en, default: cn)
# AUTOGLM_LANGUAGE=cn

# Optional: ADB Device ID
# AUTOGLM_DEVICE_ID=
